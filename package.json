{
  "name": "angular-2-daterangepicker",
  "version": "1.1.18",
  "description": "Bootstrap Daterange picker for angular2",
  "main": "index.js",
  "scripts": {
    "preinstall": "npm install -g typescript typescript-formatter lite-server concurrently rimraf",
    "start": "tsfmt -r daterangepicker-component.ts calendar-component.ts daterangepicker-module.ts index.ts daterangepicker-options.ts index.d.ts && rimraf ./dist && tsc && concurrently \"tsc -w\" \"lite-server\" ",
    "tsFormatter": "tsfmt -r daterangepicker-component.ts calendar-component.ts daterangepicker-module.ts index.ts daterangepicker-options.ts index.d.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nikhil-001mehta/angular-2-daterangepicker.git"
  },
  "keywords": [
    "angular2",
    "daterangepicker",
    "bootstrap"
  ],
  "author": "nikhil.mehta",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/nikhil-001mehta/angular-2-daterangepicker/issues"
  },
  "homepage": "https://github.com/nikhil-001mehta/angular-2-daterangepicker#readme",
  "dependencies": {
    "moment": "^2.17.1",
    "moment-range": "^2.2.0"
  },
  "devDependencies": {
    "@angular/common": "^2.4.1",
    "@angular/compiler": "^2.4.9",
    "@angular/core": "^2.4.1",
    "@angular/forms": "^2.4.1",
    "@angular/platform-browser": "^2.4.9",
    "@angular/platform-browser-dynamic": "^2.4.1",
    "@types/node": "^6.0.57",
    "concurrently": "^3.1.0",
    "core-js": "^2.4.1",
    "jquery": "^3.1.1",
    "lite-server": "^2.2.2",
    "reflect-metadata": "^0.1.8",
    "rimraf": "^2.5.4",
    "systemjs": "0.19.40",
    "typescript": "~2.0.10"
  }
}
